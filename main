-- 2. PREMIUM & BYPASS (Dosyalardan Entegre Edildi)
createSection(PremiumTab, "Gamepass Unlocker")

createButton(PremiumTab, "üîì FULL BYPASS (Her ≈ûeyi A√ß)", function()
    -- Ads Controller Bypass
    spawn(function()
        local success, Ads = pcall(function() return require(ReplicatedStorage.Modules.Client.Ads.AdvertisementsController) end)
        if success and Ads then
            Ads.GetUnlockedDuration = function() return 99999 end 
            Ads.IsEligibleForRewardedVideoAd = function() return true end
            Ads.IsRewardedVideoAdReady = function() 
                return require(ReplicatedStorage.Packages.Promise).resolve(Enum.AdAvailabilityResult.IsAvailable) 
            end
            if Ads.AllPassesFetched ~= nil then Ads.AllPassesFetched = true end
        end
    end)

    -- Gamepass Controller Bypass (GamepassUnlockController.txt)
    spawn(function()
        local success, GP = pcall(function() return require(ReplicatedStorage.Modules.Client.UI.GamepassUnlockController) end)
        if success and GP then
            GP.OwnsGamepass = function() return true end
            if GP.AllPassesFetched ~= nil then GP.AllPassesFetched = true end
        end
    end)
    
    game:GetService("StarterGui"):SetCore("SendNotification", {Title="Premium", Text="T√ºm kilitler istemci tarafƒ±nda a√ßƒ±ldƒ±!", Duration=3})
end)

createButton(PremiumTab, "üè† Ev Bekleme S√ºresini Kaldƒ±r", function()
    -- GuinRemote / RE √ºzerinden sunucuya sinyal g√∂nder
    local r = ReplicatedStorage:FindFirstChild("RE") or ReplicatedStorage:FindFirstChild("Remotes")
    if r and r:FindFirstChild("RemoteEvent") then
        pcall(function() r.RemoteEvent:FireServer("UnlockFeature", "VIP_HOUSE_COOLDOWN") end)
    end
end)

createButton(PremiumTab, "üî• Evi Anƒ±nda Yak", function()
    local possibleArgs = {
        {"HouseSettings", "Fire", true},
        {"HouseSettings", "FireOn", true},
        {"SetState", "Fire", true}
    }
    local r = ReplicatedStorage:FindFirstChild("RE") or ReplicatedStorage:FindFirstChild("Remotes")
    if r then
        local ev = r:FindFirstChild("RemoteEvent")
        for _, args in pairs(possibleArgs) do
            pcall(function() ev:FireServer(unpack(args)) end)
        end
    end
end)

