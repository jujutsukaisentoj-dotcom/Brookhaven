-- ==============================================================================
-- 1. LOADING EKRANI (Main.txt'den Alƒ±ndƒ± ve Optimize Edildi)
-- ==============================================================================

local function PlayLoading()
    local LoadGui = Instance.new("ScreenGui")
    LoadGui.Name = "XenoHubLoading"
    LoadGui.IgnoreGuiInset = true
    pcall(function() LoadGui.Parent = CoreGui end)

    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(1,0,1,0)
    Frame.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    Frame.Parent = LoadGui

    local Gradient = Instance.new("UIGradient")
    Gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(145, 90, 230)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(20, 20, 30))
    }
    Gradient.Rotation = 45
    Gradient.Parent = Frame

    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0.2, 0)
    Title.Position = UDim2.new(0, 0, 0.4, 0)
    Title.BackgroundTransparency = 1
    Title.Text = "üíé XENO HUB V4 ULTIMATE üíé"
    Title.Font = Enum.Font.GothamBlack
    Title.TextColor3 = Color3.new(1,1,1)
    Title.TextSize = 32
    Title.Parent = Frame

    local BarBG = Instance.new("Frame")
    BarBG.Size = UDim2.new(0.4, 0, 0.02, 0)
    BarBG.Position = UDim2.new(0.3, 0, 0.6, 0)
    BarBG.BackgroundColor3 = Color3.fromRGB(40,40,40)
    BarBG.Parent = Frame
    Instance.new("UICorner", BarBG).CornerRadius = UDim.new(1,0)

    local Bar = Instance.new("Frame")
    Bar.Size = UDim2.new(0, 0, 1, 0)
    Bar.BackgroundColor3 = Color3.fromRGB(160, 100, 255)
    Bar.Parent = BarBG
    Instance.new("UICorner", Bar).CornerRadius = UDim.new(1,0)

    -- Loading Animasyonu
    TweenService:Create(Bar, TweenInfo.new(2, Enum.EasingStyle.Quart), {Size = UDim2.new(1,0,1,0)}):Play()
    task.wait(2.5)
    
    TweenService:Create(Frame, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
    TweenService:Create(Title, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
    TweenService:Create(BarBG, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
    task.wait(0.5)
    LoadGui:Destroy()
end
PlayLoading()

