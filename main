    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "XenoCartolaLoading"
    ScreenGui.IgnoreGuiInset = true
    pcall(function() ScreenGui.Parent = CoreGui end)

    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(1, 0, 1, 0)
    Frame.BackgroundColor3 = Color3.fromRGB(10, 10, 12)
    Frame.Parent = ScreenGui

    local Logo = Instance.new("TextLabel")
    Logo.Size = UDim2.new(1, 0, 0.2, 0)
    Logo.Position = UDim2.new(0, 0, 0.4, 0)
    Logo.BackgroundTransparency = 1
    Logo.Text = "CARTOLA HUB"
    Logo.Font = Enum.Font.GothamBlack
    Logo.TextColor3 = Color3.fromRGB(145, 90, 230) -- Mor
    Logo.TextSize = 48
    Logo.TextTransparency = 1
    Logo.Parent = Frame

    local Sub = Instance.new("TextLabel")
    Sub.Size = UDim2.new(1, 0, 0.05, 0)
    Sub.Position = UDim2.new(0, 0, 0.5, 0)
    Sub.BackgroundTransparency = 1
    Sub.Text = "Remastered by Xeno Team"
    Sub.Font = Enum.Font.Gotham
    Sub.TextColor3 = Color3.fromRGB(180, 180, 180)
    Sub.TextSize = 18
    Sub.TextTransparency = 1
    Sub.Parent = Frame

    -- Animasyon
    TweenService:Create(Logo, TweenInfo.new(1), {TextTransparency = 0}):Play()
    task.wait(0.5)
    TweenService:Create(Sub, TweenInfo.new(1), {TextTransparency = 0}):Play()
    task.wait(2.5)
    
    TweenService:Create(Frame, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
    TweenService:Create(Logo, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
    TweenService:Create(Sub, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
    task.wait(0.5)
    ScreenGui:Destroy()
end
task.spawn(ShowLoading)

