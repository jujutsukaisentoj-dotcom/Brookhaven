-- K√ºt√ºphane ve Ana Pencere
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hexagon Full Access | Brookhaven", "DarkTheme")

-- --- üì± MOBƒ∞L A√á/KAPAT BUTONU ---
local CoreGui = game:GetService("CoreGui")
local ScreenGui = Instance.new("ScreenGui", CoreGui)
local ToggleBtn = Instance.new("TextButton", ScreenGui)
local UICorner = Instance.new("UICorner", ToggleBtn)

ToggleBtn.Size = UDim2.new(0, 55, 0, 55)
ToggleBtn.Position = UDim2.new(0.02, 0, 0.45, 0)
ToggleBtn.Text = "HEX"
ToggleBtn.Draggable = true
ToggleBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
ToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
UICorner.CornerRadius = UDim.new(1, 0)

ToggleBtn.MouseButton1Click:Connect(function() Library:ToggleUI() end)

-- --- üì° RE REMOTE BAƒûLANTISI ---
local RE_Folder = game:GetService("ReplicatedStorage"):WaitForChild("RE")

local function TriggerBrookhaven(itemName)
    -- Brookhaven'ƒ±n RE klas√∂r√ºndeki RemoteEvent'i tetikler
    local remote = RE_Folder:FindFirstChildOfClass("RemoteEvent")
    if remote then
        remote:FireServer(itemName)
        Library:Notify("RE Tetiklendi", itemName .. " ba≈üarƒ±yla istendi!", 2)
    end
end

-- --- üéí LIMITED ITEMS & MONEY GUN ---
local Items = Window:NewTab("Limited Items")
local ItemsSection = Items:NewSection("ReplicatedStorage Items")

local itemList = {
    "MoneyGun", "Glider", "Cannon", "EggLauncher", 
    "Chainsaw", "UraniumRod", "Couch", "WaterBalloon"
}

for _, item in pairs(itemList) do
    ItemsSection:NewButton("Spawn " .. item, "RE √ºzerinden " .. item .. " verir.", function()
        TriggerBrookhaven(item)
    end)
end

ItemsSection:NewButton("üî• Spawn All Items", "T√ºm listeyi RE √ºzerinden tetikler.", function()
    for _, item in pairs(itemList) do
        TriggerBrookhaven(item)
        wait(0.2)
    end
end)

-- --- ‚ú® FE PARTICLES (DEX FULL LIST) ---
local ParticleTab = Window:NewTab("FE Particles")

-- Dex Explorer'dan Gelen Ger√ßek Kategoriler ve Texture ID'leri
local particleData = {
    ["‚ù§Ô∏è Hearts"] = "rbxassetid://241377356",
    ["‚ú® Sparks"] = "rbxassetid://241386641",
    ["ü´ß Bubbles"] = "rbxassetid://243666060",
    ["üéµ Music"] = "rbxassetid://1084969841",
    ["üí® Smoke"] = "rbxassetid://243666060",
    ["‚≠ê Stars"] = "rbxassetid://241808674",
    ["üî• Fire"] = "rbxassetid://242293452",
    ["‚ö´ Dots"] = "rbxassetid://258123447"
}

local colors = {
    ["White"] = Color3.fromRGB(255, 255, 255),
    ["Yellow"] = Color3.fromRGB(255, 255, 0),
    ["Orange"] = Color3.fromRGB(255, 165, 0),
    ["Green"] = Color3.fromRGB(0, 255, 0),
    ["Blue"] = Color3.fromRGB(0, 0, 255),
    ["Purple"] = Color3.fromRGB(170, 0, 255),
    ["Pink"] = Color3.fromRGB(255, 100, 200),
    ["Red"] = Color3.fromRGB(255, 0, 0),
    ["Black"] = Color3.fromRGB(10, 10, 10)
}

-- Dinamik Men√º Olu≈üturma
for catName, texture in pairs(particleData) do
    local section = ParticleTab:NewSection(catName)
    for colorName, colorValue in pairs(colors) do
        section:NewButton(catName .. " " .. colorName, colorName .. " renginde efekt.", function()
            local char = game.Players.LocalPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                -- Mevcut efekti temizle
                for _, v in pairs(char.HumanoidRootPart:GetChildren()) do
                    if v:IsA("ParticleEmitter") then v:Destroy() end
                end
                
                -- Yeni efekt olu≈ütur (Dex Listesindeki gibi)
                local p = Instance.new("ParticleEmitter")
                p.Name = "FE_Particle_" .. colorName
                p.Texture = texture
                p.Color = ColorSequence.new(colorValue)
                p.Size = NumberSequence.new(0.7)
                p.Rate = 60
                p.Lifetime = NumberRange.new(1, 1.5)
                p.Parent = char.HumanoidRootPart
            end
        end)
    end
end

-- üö´ TEMƒ∞ZLEME
ParticleTab:NewSection("Kapat"):NewButton("üö´ T√ºm Efektleri Sil", "Karakteri temizler.", function()
    local char = game.Players.LocalPlayer.Character
    if char then
        for _, v in pairs(char.HumanoidRootPart:GetChildren()) do
            if v:IsA("ParticleEmitter") then v:Destroy() end
        end
    end
end)
