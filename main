addSection(VisualTab, "D√ºnya")
addToggle(VisualTab, "Nuke Modu (Skybox)", function(state)
    if state then
        -- Skybox animasyonlarƒ± ve karakter deƒüi≈üimleri
        local args = {123402086843885, 100839513065432, 78300682916056, 86276701020724, 78409653958165, 15093053680}
        ReplicatedStorage.Remotes.ChangeCharacterBody:InvokeServer(args)
        -- Animasyonlar (AnimationID'ler main.txt'den)
        local anim = Instance.new("Animation")
        anim.AnimationId = "rbxassetid://70883871260184"
        local track = player.Character.Humanoid:LoadAnimation(anim)
        track:Play()
    else
        ReplicatedStorage.Remotes.ResetCharacterAppearance:FireServer()
    end
end)

addButton(VisualTab, "T√ºm Kapƒ±larƒ± Sil (Delete Doors)", function()
    for _, v in pairs(Workspace:GetDescendants()) do
        if v.Name == "Door" or v.Name:find("Gate") then
            v:Destroy()
        end
    end
end)

