-- ==============================================================================
-- 1. PROFESYONEL Y√úKLEME EKRANI (main (24).txt tabanlƒ±)
-- ==============================================================================
local function ShowLoading()
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "XenoLoading"
    ScreenGui.IgnoreGuiInset = true
    ScreenGui.Parent = CoreGui

    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(1, 0, 1, 0)
    Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Frame.Parent = ScreenGui

    local Gradient = Instance.new("UIGradient")
    Gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Theme.Main),
        ColorSequenceKeypoint.new(0.5, Theme.Accent),
        ColorSequenceKeypoint.new(1, Theme.Main)
    }
    Gradient.Rotation = 90
    Gradient.Parent = Frame

    local Logo = Instance.new("TextLabel")
    Logo.Size = UDim2.new(0, 300, 0, 50)
    Logo.Position = UDim2.new(0.5, -150, 0.5, -25)
    Logo.BackgroundTransparency = 1
    Logo.Text = "XENOHUB BLACK"
    Logo.Font = Enum.Font.GothamBold
    Logo.TextSize = 40
    Logo.TextColor3 = Theme.Text
    Logo.Parent = Frame

    -- Animasyonlar
    TweenService:Create(Frame, TweenInfo.new(1.5), {BackgroundTransparency = 1}):Play()
    task.wait(2)
    ScreenGui:Destroy()
end

task.spawn(ShowLoading)

-- ==============================================================================
-- 2. ANA BYPASS Sƒ∞STEMƒ∞ (Dosyalardan Gelen Mantƒ±k)
-- ==============================================================================
local function ApplyBypasses()
    pcall(function()
        -- Reklam ve S√ºre Bypass (ExtendUnlockController.txt tabanlƒ±)
        local Ads = require(ReplicatedStorage.Modules.Client.Ads.AdvertisementsController)
        Ads.GetUnlockedDuration = function() return 999999 end
        
        -- Gamepass Kilitlerini Kaldƒ±r (GamepassUnlockController.txt tabanlƒ±)
        local GP = require(ReplicatedStorage.Modules.Client.UI.Gamepass.GamepassController)
        GP.IsUnlocked = function() return true end
        
        -- Panel Engellerini Kaldƒ±r
        local ShowGP = require(ReplicatedStorage.Modules.Client.UI.GamepassUnlockController)
        ShowGP.Show = function() return end -- Satƒ±n alma uyarƒ±sƒ±nƒ± kapat
    end)
end

ApplyBypasses()

