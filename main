CreateTitle(FreePassTab, "Free Gamepass:")

local selectedPass = "Vip"
local Gamepasses = {
    "Vip", "Premium", "Estates", "Vehicle Pack", "Land Unlocked", 
    "Boat Pack", "Vehicle Speed", "Vehicle Customization", 
    "Vehicle Upgrade", "Music Unlocked", "Disaster Pass", 
    "Penthouse", "Theme Pass", "On Fire", "Horses Unlocked", "Faces Unlocked"
}

CreateDropdown(FreePassTab, "Select Gamepass", Gamepasses, function(val)
    selectedPass = val
end)

CreateButton(FreePassTab, "Get Gamepass | AD Button needed", function()
    -- Ads Timer Fix (20 Saatlik S√ºre Ayarƒ±)
    spawn(function()
        local success, Ads = pcall(function() return require(ReplicatedStorage.Modules.Client.Ads.AdvertisementsController) end)
        if success and Ads then
            -- Oyunun "Reklamƒ± izledin" dediƒüi s√ºreyi man√ºple ediyoruz.
            -- 72000 saniye = 20 Saat
            Ads.GetUnlockedDuration = function() return 72000 end 
            
            Ads.IsEligibleForRewardedVideoAd = function() return true end
            Ads.IsRewardedVideoAdReady = function() 
                return require(ReplicatedStorage.Packages.Promise).resolve(Enum.AdAvailabilityResult.IsAvailable) 
            end
            
            -- √ñd√ºl fonksiyonunu tetikle
            if Ads.OnRewardedAdClaimed then
                Ads.OnRewardedAdClaimed:Fire("Item Refresh")
            end
        end
    end)
    
    -- G√∂rsel Kilit A√ßma
    pcall(function()
        local GPUnlock = require(ReplicatedStorage.Modules.Client.UI.GamepassUnlockController)
        local GPModule = require(ReplicatedStorage.Modules.Shared.PlayerData.Gamepasses)
        -- Se√ßili veya t√ºm√ºn√º a√ß
        for _, gpData in pairs(GPModule.All) do
            GPUnlock.Show(gpData) 
        end
    end)
    
    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Cartola Hub", Text = selectedPass .. " Aktif (20 Saat)!", Duration = 3})
end)

CreateButton(FreePassTab, "Get All Gamepasses (Best) (Mobile) (60% Visual)", function()
    -- Full Bypass (OwnsGamepass = true)
    spawn(function()
        local success, GP = pcall(function() return require(ReplicatedStorage.Modules.Client.UI.GamepassUnlockController) end)
        if success and GP then
            GP.OwnsGamepass = function() return true end
            if GP.AllPassesFetched ~= nil then GP.AllPassesFetched = true end
        end
        
        -- Kilit ƒ∞konlarƒ±nƒ± Kaldƒ±r
        local pGui = Players.LocalPlayer:WaitForChild("PlayerGui")
        for _, v in pairs(pGui:GetDescendants()) do
            if v:IsA("ImageLabel") and (v.Image:find("lock") or v.Name:lower():find("lock")) then
                v.Visible = false
            end
        end
    end)
    
    -- Remote ile Sunucuya Sinyal
    local remote = ReplicatedStorage:FindFirstChild("RE") or ReplicatedStorage:FindFirstChild("Remotes")
    if remote then
        pcall(function() remote:FindFirstChild("RemoteEvent"):FireServer("UnlockAllPasses", true) end)
    end
    
    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Full Bypass", Text = "T√ºm Gamepassler Aktif!", Duration = 5})
end)

