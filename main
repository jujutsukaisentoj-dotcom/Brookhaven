CreateButton(TrollTab, "üï≥Ô∏è Black Hole", function()
    local Att = Instance.new("Attachment", Instance.new("Part", Workspace))
    Att.Parent.Anchored = true
    Att.Parent.Transparency = 1
    Att.Parent.CanCollide = false
    
    local function Magnet(v)
        if v:IsA("BasePart") and not v.Anchored then
            local Align = Instance.new("AlignPosition", v)
            Align.Attachment0 = Instance.new("Attachment", v)
            Align.Attachment1 = Att
            Align.MaxForce = 999999999
            Align.Responsiveness = 200
        end
    end
    for _, v in pairs(Workspace:GetDescendants()) do Magnet(v) end
    
    task.spawn(function()
        while Att.Parent do
            Att.WorldCFrame = player.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-15)
            task.wait()
        end
    end)
end)

CreateButton(TrollTab, "FE Invisible", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TROLLLINUXKKK/Fe-invis-vel-/refs/heads/main/Main.txt"))()
end)

CreateButton(TrollTab, "Fling (All)", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/0866/lua/main/UniversalFling.lua"))()
end)

-- *** AVATAR ***
CreateTitle(AvatarTab, "Copy")
CreateButton(AvatarTab, "Karakteri Kopyala (Yakƒ±n)", function()
    local target = nil
    local dist = 50
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= player and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
            local d = (player.Character.HumanoidRootPart.Position - p.Character.HumanoidRootPart.Position).Magnitude
            if d < dist then target = p; dist = d end
        end
    end
    
    if target then
        local desc = target.Character.Humanoid:GetAppliedDescription()
        player.Character.Humanoid:ApplyDescription(desc)
    end
end)

-- *** SETTINGS ***
CreateButton(SettingsTab, "Kapat (Unload)", function()
    ScreenGui:Destroy()
end)

