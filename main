-- 4. TROLL & YIKIM
createSection(TrollTab, "Fizik & Eƒülence")

createButton(TrollTab, "üï≥Ô∏è Black Hole (Kara Delik)", function()
    -- Optimize edilmi≈ü Black Hole Logic
    local Folder = Instance.new("Folder", Workspace)
    local Part = Instance.new("Part", Folder)
    local Attachment1 = Instance.new("Attachment", Part)
    Part.Anchored = true; Part.CanCollide = false; Part.Transparency = 1

    local function ForcePart(v)
        if v:IsA("BasePart") and not v.Anchored and not v.Parent:FindFirstChild("Humanoid") then
            v.CanCollide = false
            local Torque = Instance.new("Torque", v)
            Torque.Torque = Vector3.new(100000, 100000, 100000)
            local Align = Instance.new("AlignPosition", v)
            local Att2 = Instance.new("Attachment", v)
            Torque.Attachment0 = Att2; Align.Attachment0 = Att2; Align.Attachment1 = Attachment1
            Align.MaxForce = math.huge; Align.Responsiveness = 200
        end
    end
    for _, v in pairs(Workspace:GetDescendants()) do ForcePart(v) end
    
    -- D√∂ng√º yerine Tween veya basit loop
    task.spawn(function()
        while Part.Parent do
            Attachment1.WorldCFrame = player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -10)
            task.wait(0.01)
        end
    end)
end)

createButton(TrollTab, "üëª FE G√∂r√ºnmezlik (Invisible)", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TROLLLINUXKKK/Fe-invis-vel-/refs/heads/main/Main.txt"))()
end)

createButton(TrollTab, "üöÅ Drone √áaƒüƒ±r", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/BRENOPOOF/drone/refs/heads/main/Main.txt"))()
