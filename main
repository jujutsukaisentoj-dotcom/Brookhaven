-- 1. XENO LOADING SCREEN (Main.txt'den Optimize Edildi)
-- ==============================================================================

local function PlayLoading()
    if _G.Loaded then return end -- Tekrar y√ºklenmesini engelle
    _G.Loaded = true

    local LoadGui = Instance.new("ScreenGui")
    LoadGui.Name = "XenoHubLoading"
    LoadGui.IgnoreGuiInset = true
    pcall(function() LoadGui.Parent = CoreGui end)

    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(1,0,1,0)
    Frame.BackgroundColor3 = Color3.fromRGB(12, 12, 18)
    Frame.Parent = LoadGui

    local ROXO = Color3.fromRGB(145, 90, 230)
    
    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0.1, 0)
    Title.Position = UDim2.new(0, 0, 0.4, 0)
    Title.BackgroundTransparency = 1
    Title.Text = "üíé XENO HUB V5 üíé"
    Title.Font = Enum.Font.GothamBlack
    Title.TextColor3 = ROXO
    Title.TextSize = 32
    Title.TextTransparency = 1
    Title.Parent = Frame

    local Sub = Instance.new("TextLabel")
    Sub.Size = UDim2.new(1, 0, 0.05, 0)
    Sub.Position = UDim2.new(0, 0, 0.5, 0)
    Sub.BackgroundTransparency = 1
    Sub.Text = "Sistemler optimize ediliyor..."
    Sub.Font = Enum.Font.Gotham
    Sub.TextColor3 = Color3.fromRGB(180, 180, 200)
    Sub.TextSize = 16
    Sub.TextTransparency = 1
    Sub.Parent = Frame

    -- Animasyon
    TweenService:Create(Title, TweenInfo.new(1), {TextTransparency = 0}):Play()
    task.wait(0.5)
    TweenService:Create(Sub, TweenInfo.new(1), {TextTransparency = 0}):Play()
    
    -- Y√ºkleme √áubuƒüu
    local BarBG = Instance.new("Frame")
    BarBG.Size = UDim2.new(0.3, 0, 0.01, 0)
    BarBG.Position = UDim2.new(0.35, 0, 0.6, 0)
    BarBG.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    BarBG.Parent = Frame
    Instance.new("UICorner", BarBG).CornerRadius = UDim.new(1,0)

    local Bar = Instance.new("Frame")
    Bar.Size = UDim2.new(0, 0, 1, 0)
    Bar.BackgroundColor3 = ROXO
    Bar.Parent = BarBG
    Instance.new("UICorner", Bar).CornerRadius = UDim.new(1,0)

    TweenService:Create(Bar, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1,0,1,0)}):Play()
    task.wait(2.2)

    -- Kapanƒ±≈ü
    TweenService:Create(Frame, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
    TweenService:Create(Title, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
    TweenService:Create(Sub, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
    TweenService:Create(BarBG, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
    TweenService:Create(Bar, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
    task.wait(0.5)
    LoadGui:Destroy()
end
PlayLoading()

