local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "XenoHub - Brookhaven Items", HidePremium = false, SaveConfig = true, ConfigFolder = "XenoHub"})

-- Sekme Olu≈üturma
local Tab = Window:MakeTab({
	Name = "E≈üyalar (Items)",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- SENƒ∞N KODLARIN BURADA BA≈ûLIYOR --

Tab:AddButton({
	Title = "üí∏ Money Gun",
	Callback = function()
		local rep = game:GetService("ReplicatedStorage")
		local Players = game:GetService("Players")
		local localPlayer = Players.LocalPlayer
		local itemName = "MoneyGun"

		local function tryNotify(title, text)
			pcall(function()
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = title,
					Text = text,
					Duration = 4
				})
			end)
		end

		local ok, err = pcall(function()
			local re = rep:FindFirstChild("RE") or rep:FindFirstChild("Remotes") or rep
			local candidates = {"GetItem", "Getƒ∞tem", "Get_Item", "GetTool", "GetToolToServer"}

			for _, name in ipairs(candidates) do
				local r = re:FindFirstChild(name)
				if r then
					if r.InvokeServer then
						r:InvokeServer(itemName)
						tryNotify("üí∏ Money Gun", "Requested via "..name.." ‚ö°")
						return
					elseif r.FireServer then
						r:FireServer(itemName)
						tryNotify("üí∏ Money Gun", "Requested via "..name.." üî•")
						return
					end
				end
			end

			local trigger = re:FindFirstChild("1Playe1rTrigge1rEven1t") or rep:FindFirstChild("1Playe1rTrigge1rEven1t")
			if trigger then
				local args = {
					[1] = "AcceptedToolToServer",
					[2] = itemName,
					[3] = localPlayer
				}

				if trigger.FireServer then
					trigger:FireServer(unpack(args))
					tryNotify("üí∏ Money Gun", "Triggered successfully üéØ")
					return
				elseif trigger.InvokeServer then
					trigger:InvokeServer(unpack(args))
					tryNotify("üí∏ Money Gun", "Triggered successfully ‚öôÔ∏è")
					return
				end
			end

			local topRemotes = rep:FindFirstChild("Remotes")
			if topRemotes then
				local r = topRemotes:FindFirstChild("GetItem") or topRemotes:FindFirstChild("Getƒ∞tem")
				if r and r.FireServer then
					r:FireServer(itemName)
					tryNotify("üí∏ Money Gun", "Requested via Remotes üì°")
					return
				end
			end

			tryNotify("üí∏ Money Gun", "No suitable remote found ‚ùå")
		end)

		if not ok then
			tryNotify("üí∏ Money Gun", "Error: "..tostring(err).." ‚ö†Ô∏è")
		end
	end
})

Tab:AddButton({
	Title = "ü™Ç Glider",
	Callback = function()
		local args = {
			[1] = "AcceptedToolToServer",
			[2] = "Glider",
			[3] = game:GetService("Players").LocalPlayer
		}
		game:GetService("ReplicatedStorage").RE:FindFirstChild("1Playe1rTrigge1rEven1t"):FireServer(unpack(args))
	end
})

