createButton(AvatarTab, "üé≠ Karakteri Kopyala", function()
    local target
    for _, p in pairs(Players:GetPlayers()) do
        if string.sub(p.Name:lower(), 1, #_G.CopyTarget) == _G.CopyTarget:lower() then
            target = p
            break
        end
    end
    if target and target.Character then
        local desc = target.Character.Humanoid:GetAppliedDescription()
        pcall(function() player.Character.Humanoid:ApplyDescription(desc) end)
    end
end)

