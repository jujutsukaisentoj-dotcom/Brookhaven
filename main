-- --- 2. TAB: FE PARTICLES (FULL DEX LIST) ---
local ParticleTab = Window:NewTab("FE Particles")
local colors = {"White", "Yellow", "Orange", "Green", "Blue", "Purple", "Pink", "Red", "Black"}
local dexCats = {"Hearts", "Sparks", "Bubble", "Music", "Smoke", "Star", "Fire", "Dots", "Twinkle"}

local EmmiterSec = ParticleTab:NewSection("âœ¨ EmmiterList KontrolÃ¼")
EmmiterSec:NewButton("ğŸ”“ Unlock EmmiterList", "Oyunun yerel emmiter listesini aÃ§ar.", function()
    AccessEmmiterList()
end)

for _, cat in pairs(dexCats) do
    local sec = ParticleTab:NewSection(cat)
    for _, col in pairs(colors) do
        sec:NewButton(cat .. " " .. col, col .. " " .. cat .. " efektini tetikler.", function()
            GiveParticle(cat, col)
            
            local root = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
            if root then
                for _, v in pairs(root:GetChildren()) do if v:IsA("ParticleEmitter") then v:Destroy() end end
                local p = Instance.new("ParticleEmitter", root)
                p.Name = "HexEffect"
                p.Color = ColorSequence.new(Color3.fromHex(col == "Black" and "000000" or col))
                p.Rate = 50
            end
        end)
    end
end

-- --- 3. TAB: AYARLAR ---
