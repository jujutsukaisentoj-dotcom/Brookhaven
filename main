createToggle(TrollTab, "üëÅÔ∏è Stalk & Kill (Takip Et)", function(state)
    _G.Stalking = state
    if not _G.TargetPlayer then return end
    task.spawn(function()
        while _G.Stalking and _G.TargetPlayer do
            local target = Players[_G.TargetPlayer]
            if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                player.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 3)
            end
            task.wait()
        end
    end)
end)

createTextBox(TrollTab, "Takip Edilecek Ki≈üi", "Kullanƒ±cƒ± Adƒ±...", function(text)
    for _, p in pairs(Players:GetPlayers()) do
        if string.sub(p.Name:lower(), 1, #text) == text:lower() then
            _G.TargetPlayer = p.Name
            break
        end
    end
end)

