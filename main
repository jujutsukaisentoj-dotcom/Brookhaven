createToggle(VisualTab, "‚ò£Ô∏è Nuke Modu (Skybox)", function(state)
    if state then
        local anim = Instance.new("Animation")
        anim.AnimationId = "rbxassetid://70883871260184"
        local track = player.Character.Humanoid:LoadAnimation(anim)
        track:Play()
    else
        pcall(function() ReplicatedStorage.Remotes.ResetCharacterAppearance:FireServer() end)
    end
end)

