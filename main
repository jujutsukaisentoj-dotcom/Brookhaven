local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hexagon Pro | Brookhaven", "DarkTheme")

-- --- ğŸ“± MOBÄ°L AÃ‡/KAPAT VE SÃœRÃœKLEME ---
local CoreGui = game:GetService("CoreGui")
local ScreenGui = Instance.new("ScreenGui", CoreGui)
local ToggleBtn = Instance.new("TextButton", ScreenGui)
local UICorner = Instance.new("UICorner", ToggleBtn)

ToggleBtn.Size = UDim2.new(0, 55, 0, 55)
ToggleBtn.Position = UDim2.new(0.02, 0, 0.45, 0)
ToggleBtn.Text = "HEX"
ToggleBtn.Draggable = true
ToggleBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
ToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleBtn.Font = Enum.Font.FredokaOne
UICorner.CornerRadius = UDim.new(1, 0)

ToggleBtn.MouseButton1Click:Connect(function() Library:ToggleUI() end)

-- --- ğŸ“¡ GÃœÃ‡LÃœ REMOTE & GUI ERÄ°ÅÄ°MÄ° ---
local rep = game:GetService("ReplicatedStorage")
local lp = game:GetService("Players").LocalPlayer
local PlayerGui = lp:WaitForChild("PlayerGui")

local function notify(title, text)
    pcall(function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = title,
            Text = text,
            Duration = 3
        })
    end)
end

-- ğŸ› ï¸ EMMITERLIST KÄ°LÄ°DÄ°NÄ° KIRMA VE TAM ERÄ°ÅÄ°M
local function UnlockEmmiterSystem()
    local path = PlayerGui:FindFirstChild("NoResetGUIHandler")
    if path then
        local holder = path:FindFirstChild("AvatarEditorHolder")
        local menu = holder and holder:FindFirstChild("AvatarEditorMenu")
        local catalog = menu and menu:FindFirstChild("Catalog")
        local container = catalog and catalog:FindFirstChild("Container")
        local emmiterList = container and container:FindFirstChild("EmmiterList")
        
        if emmiterList then
            -- Frame'i ve iÃ§eriÄŸi tamamen gÃ¶rÃ¼nÃ¼r ve aktif yap
            emmiterList.Visible = true
            if emmiterList:FindFirstChild("ScrollingFrame") then
                emmiterList.ScrollingFrame.Visible = true
            end
            
            for _, v in pairs(emmiterList:GetDescendants()) do
                if v:IsA("GuiObject") then 
                    v.Visible = true 
                end
                -- Kilitli butonlarÄ± veya Ã¶zellikleri aÃ§
                if v:IsA("BoolValue") or v:IsA("StringValue") then
                    if v.Name == "Enabled" or v.Name == "Unlocked" or v.Name == "Bought" then
                        v.Value = true
                    end
                end
            end
            notify("Sistem", "EmmiterList Tam EriÅŸim AÃ§Ä±ldÄ±! âœ…")
            return emmiterList
        end
    end
    notify("Hata", "EmmiterList Yolu BulunamadÄ±! âŒ")
end

-- ğŸ“¡ REMOTE TETÄ°KLEYÄ°CÄ° (FireServer - Herkesde GÃ¶rÃ¼nmesi Ä°Ã§in)
local function TriggerRemote(itemName)
    local re = rep:FindFirstChild("RE")
    local trigger = re and re:FindFirstChild("1Playe1rTrigge1rEven1t")
    
    if trigger then
        -- Brookhaven'Ä±n sunucu taraflÄ± eÅŸya verme eventi
        trigger:FireServer("AcceptedToolToServer", itemName, lp)
        notify("BaÅŸarÄ±lÄ±", itemName .. " Tetiklendi! ğŸ”¥")
    else
        local fallback = rep:FindFirstChild("1Too1l")
        if fallback and fallback:IsA("RemoteFunction") then
            fallback:InvokeServer("PickingTools", itemName)
        end
    end
end

-- ParÃ§acÄ±klarÄ± Sunucuda Tetikleme Fonksiyonu
local function ServerParticle(particleName, colorName)
    local re = rep:FindFirstChild("RE")
    local pEvent = re and re:FindFirstChildOfClass("RemoteEvent")
    
    if pEvent then
        -- Brookhaven sunucuya parÃ§acÄ±k bilgisini gÃ¶nderir (Herkesde GÃ¶rÃ¼nÃ¼r)
        pEvent:FireServer("ParticleGiver", particleName, colorName)
        return true
    end
    return false
end

-- --- ğŸ’ TAB 1: LIMITED ITEMS ---
local ItemsTab = Window:NewTab("Limited Items")
local ItemsSec = ItemsTab:NewSection("Gizli EÅŸyalar (FE)")

local items = {
    {Name = "Money Gun", ID = "MoneyGun"},
    {Name = "Glider", ID = "Glider"},
    {Name = "Scythe", ID = "Scythe"},
    {Name = "Chainsaw", ID = "Chainsaw"},
    {Name = "Uranium Rod", ID = "UraniumRod"},
    {Name = "Egg Launcher", ID = "EggLauncher"},
    {Name = "Cannon", ID = "Cannon"}
}

for _, item in pairs(items) do
    ItemsSec:NewButton("ğŸ’¸ " .. item.Name, "Herkesin gÃ¶receÄŸi ÅŸekilde eÅŸyayÄ± verir.", function()
        TriggerRemote(item.ID)
    end)
end

-- --- âœ¨ TAB 2: GeliÅŸmiÅŸ EMMITER LIST (FE & SERVER) ---
local ParticleTab = Window:NewTab("Emmiter List")
local colors = {
    ["Red"] = Color3.fromRGB(255, 0, 0),
    ["Blue"] = Color3.fromRGB(0, 150, 255),
    ["Green"] = Color3.fromRGB(0, 255, 0),
    ["Purple"] = Color3.fromRGB(170, 0, 255),
    ["Yellow"] = Color3.fromRGB(255, 255, 0),
    ["Black"] = Color3.fromRGB(15, 15, 15),
    ["White"] = Color3.fromRGB(255, 255, 255)
}

local emitters = {
    {Name = "Small Hearts", ID = "2415370814", ServerName = "Hearts"},
    {Name = "Sparks", ID = "242910831", ServerName = "Sparks"},
    {Name = "Bubble", ID = "2412933758", ServerName = "Bubble"},
    {Name = "Music", ID = "2412921852", ServerName = "Music"},
    {Name = "Smoke", ID = "242907474", ServerName = "Smoke"},
    {Name = "Star", ID = "2412918458", ServerName = "Star"},
    {Name = "Fire", ID = "242908750", ServerName = "Fire"},
    {Name = "Dots", ID = "2412920230", ServerName = "Dots"},
    {Name = "Twinkle", ID = "2412915806", ServerName = "Twinkle"}
}

local EmmiterSec = ParticleTab:NewSection("ğŸ”“ Sistem EriÅŸimi")
EmmiterSec:NewButton("Unlock Full EmmiterList", "TÃ¼m kilitleri aÃ§ar ve GUI'yi gÃ¶rÃ¼nÃ¼r yapar.", function()
    UnlockEmmiterSystem()
end)

for _, emit in pairs(emitters) do
    local section = ParticleTab:NewSection(emit.Name)
    for colorName, colorValue in pairs(colors) do
        section:NewButton(emit.Name .. " [" .. colorName .. "]", "Herkesde gÃ¶rÃ¼nÃ¼r ÅŸekilde kuÅŸanÄ±r.", function()
            -- 1. Sunucu TaraflÄ± Tetikleme (DiÄŸer oyuncularÄ±n gÃ¶rmesi iÃ§in)
            ServerParticle(emit.ServerName, colorName)
            
            -- 2. Yerel GÃ¶rsel Destek (AnlÄ±k geri bildirim iÃ§in)
            local char = lp.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                for _, v in pairs(char.HumanoidRootPart:GetChildren()) do
                    if v:IsA("ParticleEmitter") then v:Destroy() end
                end
                
                local p = Instance.new("ParticleEmitter")
                p.Name = "FE_Emmiter_" .. emit.ServerName
                p.Texture = "rbxassetid://" .. emit.ID
                p.Color = ColorSequence.new(colorValue)
                p.Rate = 90
                p.Lifetime = NumberRange.new(1.5, 3)
                p.Speed = NumberRange.new(4, 8)
                p.VelocityInheritance = 0.5
                p.Parent = char.HumanoidRootPart
                p.Enabled = true
            end
        end)
    end
end

-- --- âš™ï¸ TAB 3: AYARLAR ---
local Settings = Window:NewTab("Ayarlar")
Settings:NewSection("Efekt KontrolÃ¼")
Settings:NewButton("ğŸš« TÃ¼m ParÃ§acÄ±klarÄ± Temizle", "Hem yerel hem sunucu efektlerini durdurur.", function()
    ServerParticle("None", "White") -- Sunucuya temizleme mesajÄ± gÃ¶nderir (varsa)
    if lp.Character then
        for _, v in pairs(lp.Character.HumanoidRootPart:GetChildren()) do
            if v:IsA("ParticleEmitter") then v:Destroy() end
        end
    end
end)
