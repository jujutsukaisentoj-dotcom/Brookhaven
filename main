local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Brookhaven Script", "BloodTheme") -- Buradan tema değişir

local Tab = Window:NewTab("Hileler")
local Section = Tab:NewSection("Genel")

Section:NewButton("Zıplama Artır", "Daha yükseğe zıpla", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
end)
