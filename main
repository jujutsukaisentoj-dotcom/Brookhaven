CreateTitle(VehiclesTab, "Car Mods")
CreateButton(VehiclesTab, "Speed 200 (All Cars)", function()
    for _, v in pairs(Workspace.Vehicles:GetChildren()) do
        local seat = v:FindFirstChild("VehicleSeat", true)
        if seat then seat.MaxSpeed = 200; seat.Torque = 100000 end
    end
end)

CreateToggle(VehiclesTab, "Rainbow Car (RGB)", function(state)
    _G.CarRGB = state
    task.spawn(function()
        while _G.CarRGB do
            local seat = player.Character.Humanoid.SeatPart
            if seat then
                local color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                pcall(function() ReplicatedStorage.RE["1Player1sCa1r"]:FireServer("PickingCarColor", color) end)
            end
            task.wait(0.1)
        end
    end)
end)

