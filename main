local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Brookhaven VIP Hub | Rayfield",
   LoadingTitle = "Sistem Yükleniyor...",
   LoadingSubtitle = "Hazırlayan: Gemini",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "BrookhavenRayfield",
      FileName = "Ayarlar"
   },
   KeySystem = false -- Şifre sormasın, direkt açılsın.
})

-- ANA SEKME
local MainTab = Window:CreateTab("Karakter Ayarları", 4483362458)

local SpeedSlider = MainTab:CreateSlider({
   Name = "Yürüme Hızı",
   Info = "Karakterin hızını ayarlar.",
   Range = {16, 300},
   Increment = 1,
   Suffix = "Hız",
   CurrentValue = 16,
   Flag = "WalkSpeedSlider",
   Callback = function(Value)
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
   end,
})

local JumpSlider = MainTab:CreateSlider({
   Name = "Zıplama Gücü",
   Info = "Zıplama yüksekliğini ayarlar.",
   Range = {50, 400},
   Increment = 1,
   Suffix = "Güç",
   CurrentValue = 50,
   Flag = "JumpPowerSlider",
   Callback = function(Value)
      game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
      game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
   end,
})

-- IŞINLANMA SEKMESİ
local TP = Window:CreateTab("Işınlanma", 4483362458)

TP:CreateButton({
   Name = "Banka Kasasına Işınlan",
   Callback = function()
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-442, 23, -343)
      Rayfield:Notify({Title = "Işınlandı", Content = "Banka kasasındasın!", Duration = 2})
   end,
})

TP:CreateButton({
   Name = "Hastaneye Işınlan",
   Callback = function()
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-480, 25, -75)
   end,
})

-- BİLDİRİM
Rayfield:Notify({
   Title = "Sistem Aktif!",
   Content = "Rayfield Menü Başarıyla Açıldı.",
   Duration = 5,
   Image = 4483362458,
})
