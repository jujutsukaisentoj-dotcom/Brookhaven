-- AVATAR COPY TAB (Main.txt'den)
addSection(AvatarTab, "Kopyalama")
addBox(AvatarTab, "Oyuncu Adƒ± (Kopyalanacak)", function(text)
    _G.CopyTarget = text
end)

addButton(AvatarTab, "Avatarƒ± Kopyala (Full)", function()
    local target
    for _, p in pairs(Players:GetPlayers()) do
        if string.sub(p.Name:lower(), 1, #_G.CopyTarget) == _G.CopyTarget:lower() then
            target = p
            break
        end
    end

    if target and target.Character then
        local desc = target.Character.Humanoid:GetAppliedDescription()
        pcall(function()
            player.Character.Humanoid:ApplyDescription(desc)
        end)
    end
end)

