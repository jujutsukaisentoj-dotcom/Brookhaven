Tab:AddButton({
	Title = "üç≠ Lollipop Peppermint",
	Callback = function()
		local Players = game:GetService("Players")
		local ReplicatedStorage = game:GetService("ReplicatedStorage")
		local LocalPlayer = Players.LocalPlayer

		local possibleNames = {
			"LollipopPeppermint",
			"Lollipop_Peppermint",
			"Lollipop",
			"PeppermintLollipop"
		}

		local re = ReplicatedStorage:FindFirstChild("RE")
		local trigger = re and re:FindFirstChild("1Playe1rTrigge1rEven1t")
		if not trigger then return end

		for _, itemName in ipairs(possibleNames) do
			pcall(function()
				trigger:FireServer("AcceptedToolToServer", itemName, LocalPlayer)
			end)
		end
	end
})

