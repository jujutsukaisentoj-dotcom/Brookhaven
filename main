addButton(Premium, "T√ºm Gamepassleri Aktif Et (Bypass)", function()
    -- GamepassUnlockController.txt mantƒ±ƒüƒ±
    pcall(function()
        local mod = require(ReplicatedStorage.Modules.Client.UI.GamepassUnlockController)
        mod.OwnsGamepass = function() return true end
        mod.AllPassesFetched = true
    end)
    
    -- Reklam/Extend Logic (ExtendUnlockController.txt)
    pcall(function()
        local ads = require(ReplicatedStorage.Modules.Client.Ads.AdvertisementsController)
        ads.GetUnlockedDuration = function() return 72000 end -- 20 Saat
        ads.IsEligibleForRewardedVideoAd = function() return true end
    end)
end)

